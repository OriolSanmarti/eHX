FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    BCWALL_3
    {
        //type            fixedValue;
        //value           uniform 453;
        type            externalWallHeatFluxTemperature;
        mode            flux; //HTC
        q               constant 30000; // W/m2
        kappaMethod     fluidThermo;
        value           $internalField;

    }

    ".*"
    {
        type            zeroGradient;
    }
}


